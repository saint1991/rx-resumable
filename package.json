{
  "name": "rx-resumable.js",
  "version": "1.0.0",
  "description": "RxJS support for Resumable.js",
  "main": "main.js",
  "scripts": {
    "build": "webpack --mode=production --node-env=production --config webpack.config.ts",
    "build:dev": "webpack --mode=development --config webpack.config.ts",
    "build:prod": "webpack --mode=production --node-env=production --config webpack.config.ts",
    "format": "rome format --write --indent-style=space --indent-size=2 --line-width=160 --quote-style=single ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saint1991/rx-resumablejs.git"
  },
  "keywords": [
    "RxJS",
    "resumable.js"
  ],
  "author": "saint1991",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saint1991/rx-resumablejs/issues"
  },
  "homepage": "https://github.com/saint1991/rx-resumablejs#readme",
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@types/webpack": "^5.28.0",
    "@webpack-cli/generators": "^2.5.0",
    "rome": "^0.7.0-next",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "resumablejs": "^1.1.0",
    "rxjs": "^7.5.5"
  }
